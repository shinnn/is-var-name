    "variable",
    "check",
    "character",
    "javascript",
    "cmascript",
    "specification",
    "validation",
    "spec",
    "string",
    "browser",
    "client-side"
  ],
  "devDependencies": {
    "eslint": "^0.9.2",
    "istanbul": "^0.3.2",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.7.3",
    "require-bower-files": "^1.0.0",
    "tap-spec": "^2.1.0",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98
