{
  "name": "is-var-name",
  "version": "0.0.0",
  "description": "Check if a string can be used as a JavaScript variable name",
  "repository": "shinnn/is-var-name",
  "scripts": {
    "pretest": "eslint *.js & jscs *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/is-var-name/blob/master/LICENSE"
    }
  ],
  "main": "is-var-name-cjs.js",
  "files": [
    "is-var-name-cjs.js",
    "LICENSE"
  ],
  "keywords": [
