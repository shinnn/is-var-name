{
	"name": "is-var-name",
	"version": "2.0.0",
	"description": "Check if a string can be used as a JavaScript variable name",
	"repository": "shinnn/is-var-name",
	"scripts": {
		"prebuild": "eslint --fix --format=codeframe index.mjs test.js",
		"build": "rollup --config=node:module",
		"pretest": "npm run-script build",
		"test": "nyc --reporter=html --reporter=text node test.js"
	},
	"license": "ISC",
	"files": [
		"index.js",
		"index.mjs"
	],
	"module": "index.mjs",
	"keywords": [
		"var",
		"variable",
		"let",
		"const",
		"identifier",
		"check",
		"character",
		"javascript",
		"ecmascript",
		"specification",
		"validation",
		"spec",
		"string"
	],
	"devDependencies": {
		"@shinnn/eslint-config": "^5.4.0",
		"eslint": "^4.18.0",
		"nyc": "^11.5.0",
		"rollup": "^0.56.2",
		"rollup-config-module": "^2.0.0",
		"tape": "^4.9.0"
	},
	"eslintConfig": {
		"extends": "@shinnn",
		"rules": {
			"no-new-func": "off"
		}
	}
}
